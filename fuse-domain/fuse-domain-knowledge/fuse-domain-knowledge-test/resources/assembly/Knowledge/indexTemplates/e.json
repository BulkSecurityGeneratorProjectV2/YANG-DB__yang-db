{
  "order": 0,
  "template": "e*",
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "refresh_interval": "100ms",
      "analysis": {
        "normalizer": {
          "lowercase_normalizer": {
            "filter": [
              "lowercase"
            ],
            "type": "custom"
          }
        },
        "analyzer": {
          "knowledge_ngram_analyzer": {
            "type": "custom",
            "tokenizer": "knowledge_ngram_tokenizer",
            "filter": [
              "standard",
              "knowledge_stopwords",
              "lowercase"
            ]
          },
          "knowledge_standard_analyzer": {
            "type": "custom",
            "tokenizer": "standard",
            "filter": [
              "standard",
              "knowledge_stopwords",
              "lowercase"
            ]
          }
        },
        "tokenizer": {
          "knowledge_ngram_tokenizer": {
            "type": "nGram",
            "min_gram": "1",
            "max_gram": "10",
            "token_chars": [
              "letter",
              "digit",
              "punctuation"
            ]
          }
        },
        "filter": {
          "knowledge_stopwords": {
            "type": "stop",
            "stopwords": "_none"
          }
        }
      }
    }
  },
  "mappings": {
    "pge": {
      "_all": {
        "enabled": false
      },
      "properties": {
        "type": {
          "type": "keyword"
        },
        "logicalId": {
          "type": "keyword"
        },
        "context": {
          "type": "keyword"
        },
        "category": {
          "type": "keyword"
        },
        "parentCategory": {
          "type": "keyword"
        },
        "refs": {
          "type": "keyword"
        },
        "name": {
          "type": "keyword"
        },
        "path": {
          "type": "keyword"
        },
        "mimeType": {
          "type": "keyword"
        },
        "entityId": {
          "type": "keyword"
        },
        "bdt": {
          "type": "keyword"
        },
        "fieldId": {
          "type": "keyword"
        },
        "integerValue": {
          "type": "long"
        },
        "geoValue": {
          "type": "geo_shape"
        },
        "dateValue": {
          "format": "epoch_millis||strict_date_optional_time||yyyy-MM-dd HH:mm:ss.SSS",
          "type": "date"
        },
        "stringValue": {
          "type": "text",
          "analyzer": "knowledge_ngram_analyzer",
          "search_analyzer": "knowledge_standard_analyzer",
          "fields": {
            "keyword": {
              "normalizer": "lowercase_normalizer",
              "type": "keyword"
            }
          }
        },
        "description": {
          "type": "text",
          "analyzer": "knowledge_ngram_analyzer",
          "search_analyzer": "knowledge_standard_analyzer",
          "fields": {
            "keyword": {
              "normalizer": "lowercase_normalizer",
              "type": "keyword"
            }
          }
        },
        "displayName": {
          "type": "text",
          "analyzer": "knowledge_ngram_analyzer",
          "search_analyzer": "knowledge_standard_analyzer",
          "fields": {
            "keyword": {
              "normalizer": "lowercase_normalizer",
              "type": "keyword"
            }
          }
        },
        "relationId": {
          "type": "keyword"
        },
        "entityAId": {
          "type": "keyword"
        },
        "entityALogicalId": {
          "type": "keyword"
        },
        "entityBId": {
          "type": "keyword"
        },
        "entityBLogicalId": {
          "type": "keyword"
        },
        "entityACategory": {
          "type": "keyword"
        },
        "entityBCategory": {
          "type": "keyword"
        },
        "direction": {
          "type": "keyword"
        },
        "insightId": {
          "type": "keyword"
        },
        "creationUser": {
          "type": "keyword"
        },
        "creationTime": {
          "format": "epoch_millis||strict_date_optional_time||yyyy-MM-dd HH:mm:ss.SSS",
          "type": "date"
        },
        "lastUpdateUser": {
          "type": "keyword"
        },
        "lastUpdateTime": {
          "format": "epoch_millis||strict_date_optional_time||yyyy-MM-dd HH:mm:ss.SSS",
          "type": "date"
        },
        "systemCreationTime": {
          "format": "epoch_millis||strict_date_optional_time||yyyy-MM-dd HH:mm:ss.SSS",
          "type": "date"
        },
        "systemCreationUser": {
          "type": "keyword"
        },
        "deleteUser": {
          "type": "keyword"
        },
        "deleteTime": {
          "format": "epoch_millis||strict_date_optional_time||yyyy-MM-dd HH:mm:ss.SSS",
          "type": "date"
        },
        "authorization": {
          "type": "keyword"
        },
        "authorizationCount": {
          "type": "integer"
        },
        "isExternal": {
          "type": "keyword"
        }
      }
    }
  },
  "aliases": {}
}